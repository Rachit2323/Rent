{
    "version": 2,
    "builds": [
      {
        "src": "server.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "controllers/**",
            "middleware/**",
            "models/**",
            "routes/**",
            "services/**",
            "db.ts",
            "mockData.ts"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.ts"
      }
    ]
  }
  